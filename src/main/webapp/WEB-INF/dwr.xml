<!DOCTYPE dwr PUBLIC
        "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
        "http://directwebremoting.org/schema/dwr30.dtd">

<dwr>

    <!-- init is only needed if you are extending DWR -->


    <!-- without allow, DWR isn't allowed to do anything -->
    <allow>


        <create creator="new" javascript="AssignProgress">
            <param name="class" value="com.smartzhe.dwr.AssignProgress"/>
        </create>

        <convert converter="bean" match="com.smartzhe.dwr.ProgressResult"/>
    </allow>



</dwr>